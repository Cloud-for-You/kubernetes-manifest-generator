ARGOCD:
  - name: argocd-deployment-sys
    version: v0.8.2
    type: helm
    appconfig:
      spec:
        destination:
          namespace: csas-argocd-sys
  - name: argocd-deployment-app
    version: v0.8.2
    type: helm
    appconfig:
      spec:
        destination:
          namespace: csas-argocd-app
CLUSTER_CONFIG:
  - name: cluster-config
    version: v0.1.1
    type: helm
    appconfig:
      spec:
        syncPolicy:
          syncOptions:
          - CreateNamespace=false
        destination:
          namespace: csas-argocd-sys
CSAS_OPERATOR:
  - name: csas-project-operator
    version: 1.0.0-rc.17
    type: kustomize
  - name: csas-application-operator
    version: 1.1.0-rc.11
    type: kustomize
ELOS_MONITORING:
  - name: monitoring-operator
    version: 0.1.0
    type: helm
    appconfig:
      spec:
        destination:
          namespace: csas-monitoring
  - name: monitoring
    version: 0.1.0
    type: helm
    appconfig:
      spec:
        destination:
          namespace: csas-monitoring
