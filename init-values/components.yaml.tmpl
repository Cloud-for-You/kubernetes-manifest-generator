ARGOCD:
  - name: argocd-deployment-sys
    version: v1.1.4
    type: helm
    namespace: csas-argocd-sys
    appconfig:
      spec:
        syncPolicy:
          automated: null
  - name: argocd-deployment-app
    version: v1.1.4
    type: helm
    namespace: csas-argocd-app
CLUSTER_CONFIG:
  - name: cluster-config
    version: v0.1.6
    type: helm
    namespace: csas-argocd-sys
    appconfig:
      spec:
        syncPolicy:
          automated: null
          syncOptions:
          - CreateNamespace=false
CUSTOM:
  - name: custom-resources
    type: plain
    namespace: csas-argocd-sys
    appconfig:
      spec:
        syncPolicy:
          syncOptions:
          - CreateNamespace=false
CSAS_OPERATOR:
  - name: csas-project-operator
    version: 1.0.6
    type: kustomize
  - name: csas-application-operator
    version: 1.2.6
    type: kustomize
MONITORING:
  - name: monitoring
    version: 0.2.2
    type: helm
    vendor: elostech
    namespace: csas-monitoring
  - name: dynatrace-oneagent-operator
    vendor: dynatrace
    version: v0.9.5 
    type: helm
    namespace: dynatrace
    appconfig:
      spec:
        ignoreDifferences:
          - kind: ServiceAccount
            name: dynatrace-oneagent-operator
            namespace: dynatrace
            jsonPointers:
              - /imagePullSecrets
          - kind: ServiceAccount
            name: dynatrace-oneagent
            namespace: dynatrace
            jsonPointers:
              - /imagePullSecrets
SEALED_SECRETS:
  - name: sealed-secrets-controller
    artefact: sealed-secrets
    version: 0.3.0
    type: helm
    namespace: csas-sealed-secrets
